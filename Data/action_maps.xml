<?xml version="1.0"?>
<ActionMaps>
	<ActionMap name="ui_input" passthrough="false">
		<ActionSignal native_input="kp:W" signal="ui_prev" />
		<ActionSignal native_input="kp:S" signal="ui_next" />
		<ActionSignal native_input="kp:Enter" signal="ui_action" />
	</ActionMap>
	<ActionMap name="game_input" passthrough="false">
		<ActionSignal native_input="kp:W" signal="player_jump" />
		<ActionSignal native_input="kp:D" signal="move_right" />
		<ActionSignal native_input="kr:D" signal="stop_move_right" />
		<ActionSignal native_input="kp:A" signal="move_left" />
		<ActionSignal native_input="kr:A" signal="stop_move_left" />
		<ActionSignal native_input="kp:Space" signal="player_attack" />
		<ActionSignal native_input="kp:F" signal="player_time_control" />
	</ActionMap>	
</ActionMaps>
